    "@vueuse/integrations": "^13.5.0",
    "axios": "^1.13.1",
    "bootstrap-icons": "^1.13.1",
    "chart.js": "^4.4.7",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21",
    "quasar": "^2.16.0",
    "uuid": "^11.1.0",
    "vue": "^3.4.18",
    "vue-chartjs": "^5.3.2",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
